
<div class="col-md-12">
    <% if(Game.length) {
       Game.forEach(function (Game) { 

    %>

   <div class="card b-1 hover-shadow mb-20">
        <div class="media card-body">
            <div class="media-left pr-12">
                <img class="avatar avatar-xl no-radius" src="https://www.imgworlds.com/wp-content/themes/IMG2019/img/favicon/og-thumb.png" alt="...">
            </div>
            <div class="media-body">
                <div class="mb-2">
                    <span class="fs-20 pr-16"><h2><%= Game.name ;%></h2></span>
                </div>
                <small class="fs-16 fw-300 ls-1">Còn 12/100 máy</small>
            </div>
            <div class="media-right text-right d-none d-md-block">
                <p class="fs-14 text-fade mb-12"><i class="fa fa-map-marker pr-1"></i> <%= Game.category ;%></p>
                <span class="text-fade"><i class="fa fa-money pr-1"></i><%= Game.price ;%>đ/h</span>
            </div>
        </div>
        <footer class="card-footer flexbox align-items-center">
            <div>
                <strong>Thêm vào:</strong>
                <span>12/01/2021</span>
            </div>
            <div class="card-hover-show">
                <a class="btn btn-xs fs-10 btn-bold btn-info" href="GamesRegistered/deil">View</a>
                <a class="btn btn-xs fs-10 btn-bold btn-primary" href="#" data-toggle="modal" data-target="#modal-contact">Message</a>
                <a class="btn btn-xs fs-10 btn-bold btn-warning" href="#">Delete</a>
            </div>
        </footer>
    </div>
    <% });
   }
